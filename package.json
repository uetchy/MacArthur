{
  "name": "MacArthur",
  "productName": "MacArthur",
  "version": "1.0.0",
  "description": "MacArthur brings GUI to ghq.",
  "author": "Yasuaki Uechi <uetchy@randompaper.co> (https://randompaper.co/)",
  "license": "MIT",
  "private": true,
  "main": ".",
  "scripts": {
    "test": "xo",
    "start": "npm-run-all watch:*",
    "watch:app": "cd app; npm start",
    "watch:electron": "electron .",
    "build": "electron-packager . $npm_package_productName --platform=darwin --arch=x64 --out=dist"
  },
  "dependencies": {
    "babel-register": "^6.11.5",
    "bluebird": "^3.4.1",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "electron-packager": "^7.3.0",
    "electron-prebuilt": "^1.2.8",
    "npm-run-all": "^2.3.0",
    "babel-cli": "^6.11.4",
    "babel-preset-react": "6.11.1",
    "webpack": "^1.13.1",
    "eslint-config-xo-react": "^0.8.0",
    "eslint-plugin-react": "^5.2.2",
    "xo": "^0.16.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ]
  },
  "xo": {
    "esnext": true,
    "semicolon": false,
    "envs": [
      "node",
      "browser"
    ],
    "extends": "xo-react"
  }
