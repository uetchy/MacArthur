{
  "name": "MacArthur",
  "productName": "MacArthur",
  "version": "1.0.0",
  "description": "MacArthur brings GUI to ghq.",
  "author": "Yasuaki Uechi <uetchy@randompaper.co> (https://randompaper.co/)",
  "license": "MIT",
  "private": true,
  "main": ".",
  "electronVersion": "1.3.3",
  "scripts": {
    "test": "ava && xo",
    "start": "node node_modules/react-native-macos/local-cli/cli.js start",
    "release": "electron-packager . $npm_package_productName --out=dist --ignore='^/dist$' --prune --asar --platform=darwin --arch=x64 --version=$npm_package_electronVersion --app-version=$npm_package_version --icon=./icon.icns --overwrite"
  },
  "dependencies": {
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.11.6",
    "bluebird": "^3.4.1",
    "material-ui": "^0.15.4",
    "react": "^15.3.1",
    "react-native-macos": "^0.10.0-rc2",
    "react-dom": "^15.3.1",
    "react-tap-event-plugin": "^1.0.0"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "electron": "^1.3.3",
    "electron-packager": "^7.6.0",
    "eslint-config-xo-react": "^0.9.0",
    "eslint-plugin-react": "^6.1.2",
    "npm-run-all": "^3.0.0",
    "spectron": "^3.3.0",
    "xo": "^0.16.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "xo": {
    "semicolon": false,
    "envs": [
      "browser"
    ],
    "extends": "xo-react"
  }
}
